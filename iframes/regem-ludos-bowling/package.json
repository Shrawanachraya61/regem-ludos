{
  "name": "regem-ludos-bowling",
  "version": "1.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn && yarn mv && cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "mv": "mkdir -p dist && cp -t dist/ public/* -v && rm -rf dist/main.js*",
    "clean": "rm -rf dist",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true rollup -c rollup.config.ts"
  },
  "dependencies": {
    "javascript-state-machine": "^3.1.0",
    "matter-js": "^0.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^3.0.0",
    "@types/matter-js": "^0.17.5",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "rollup": "^2.40.0",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-react-svg": "^3.0.3",
    "rollup-plugin-serve-proxy": "^1.1.4",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.2.2"
  }
}
