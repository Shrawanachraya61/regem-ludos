{
  "name": "js13kserver",
  "version": "1.0.2",
  "description": "js13kgames game jam server",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node index.js dist",
    "start:dev": "node index.js",
    "test": "mocha test",
    "watch:client": "tsc --project tsconfig.client.json --watch",
    "watch:server": "tsc --project tsconfig.server.json --watch",
    "build": "tsc --project tsconfig.client.json && tsc --project tsconfig.server.json && yarn build:course && node scripts/build.js",
    "build:script": "node scripts/build.js",
    "build:course": "node maps/compile.js",
    "dist": "node index.js dist"
  },
  "keywords": [
    "js13kgames",
    "server"
  ],
  "nodemonConfig": {
    "ext": "js,css,html"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mysql2": "^2.2.5",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "socket.io": "^4.1.3",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^11.9.4",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "concurrently": "^6.2.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-minifier": "^4.0.0",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12",
    "prettier": "^2.0.5",
    "terser": "^5.7.0",
    "typescript": "^3.9.3",
    "uglifycss": "0.0.29"
  }
}
